{
  "extends": [
    "airbnb"
  ],
  "plugins": [
    "mocha",
    "react",
    "jsx-a11y",
    "import",
    "chai-expect"
  ],
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "import/no-extraneous-dependencies": ["error", {"devDependencies": ["**/webpack.*.js", "**/*.spec.js"]}],
    "chai-expect/missing-assertion": 2
  }
}
